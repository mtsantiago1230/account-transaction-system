# Environment template file
# Copy this to .env and fill in your actual values

# Database Configuration (Option 1: Use DATABASE_URL)
DATABASE_URL=postgresql://username:password@localhost:5432/account_transaction_db

# Database Configuration (Option 2: Use individual variables)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=account_transaction_db

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=1h

# Application Configuration
PORT=3000
NODE_ENV=development